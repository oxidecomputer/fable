    "mousetrap": "^1.6.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "table": "^6.8.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-toml": "^0.5.0",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
